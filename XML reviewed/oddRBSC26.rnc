  }
sh_damageSpan =
  
  ## (damaged span of text) marks the beginning of a longer sequence of text which is damaged in some way but still legible. [11.3.3.1. ]
  element damageSpan {
    empty
    >> sch:pattern [
         id = "damageSpan-constraint-spanTo"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:damageSpan"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@spanTo"
             "\x{a}" ~
             "The @spanTo attribute of "
             sch:name [ ]
             " is required."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "damageSpan-constraint-spanTo_fr"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:damageSpan"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@spanTo"
             "L'attribut spanTo est requis."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    sh_att.global.attributes,
    sh_att.damaged.attributes,
    sh_att.typed.attributes,
    sh_att.spanning.attributes,
    empty
  }
sh_delSpan =
  
  ## (deleted span of text) marks the beginning of a longer sequence of text deleted, marked as deleted, or otherwise signaled as superfluous or spurious by an author, scribe, annotator, or corrector. [11.3.1.4. ]
  element delSpan {
    empty
    >> sch:pattern [
         id = "delSpan-constraint-spanTo"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:delSpan"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@spanTo"
             "The spanTo= attribute of "
             sch:name [ ]
             " is required."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "delSpan-constraint-spanTo_fr"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:delSpan"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@spanTo"
             "L'attribut spanTo est requis."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    sh_att.global.attributes,
    sh_att.transcriptional.attributes,
    sh_att.typed.attributes,
    sh_att.spanning.attributes,
    empty
  }
sh_ex =
  
  ## (editorial expansion) contains a sequence of letters added by an editor or transcriber when expanding an abbreviation. [11.3.1.2. ]
  element ex {
    sh_macro.xtext,
    sh_att.global.attributes,
    sh_att.editLike.attributes,
    empty
  }
sh_fw =
  
  ## (forme work) contains a running head (e.g. a header, footer), catchword, or similar material appearing on the current page. [11.6. ]
  element fw {
    sh_macro.phraseSeq,
    sh_att.global.attributes,
    sh_att.placement.attributes,
    
    ##
    attribute type {
      
      ##
      "pagenumber"
      | 
        ##
        "catchword"
      | 
        ##
        "signature"
      | 
        ##
        "header"
      | 
        ##
        "footer"
      | 
        ##
        "altpagenumber"
      | 
        ##
        "enumeration"
    }?,
    empty
  }
sh_handNotes =
  
  ## contains one or more handNote elements documenting the different hands identified within the source texts. [11.3.2.1. ]
  element handNotes { sh_handNote+, sh_att.global.attributes, empty }
sh_handShift =
  
  ## marks the beginning of a sequence of text written in a new hand, or the beginning of a scribal stint. [11.3.2.1. ]
  element handShift {
    empty,
    sh_att.global.attributes,
    sh_att.handFeatures.attributes,
    sh_att.responsibility.attributes,
    
    ## identifies the new hand.
    attribute new { sh_data.pointer }?,
    empty
  }
sh_am =
  
  ## (abbreviation marker) contains a sequence of letters or signs present in an abbreviation which are omitted or replaced in the expanded form of the abbreviation. [11.3.1.2. ]
  element am {
    (text | sh_model.gLike | sh_model.pPart.transcriptional)*,
    sh_att.global.attributes,
    sh_att.editLike.attributes,
    empty
  }
sh_restore =
  
  ## indicates restoration of text to an earlier state by cancellation of an editorial or authorial marking or instruction. [11.3.1.6. ]
  element restore {
    sh_macro.paraContent,
    sh_att.global.attributes,
    sh_att.transcriptional.attributes,
    sh_att.typed.attributes,
    empty
  }
sh_space =
  
  ## indicates the location of a significant space in the copy text. [11.5.1. ]
  element space {
    (sh_model.descLike | sh_model.certLike)*,
    sh_att.global.attributes,
    sh_att.dimensions.attributes,
    
    ## (dimension) indicates whether the space is horizontal or vertical.
    attribute dim {
      
      ## the space is horizontal.
      "horizontal"
      | 
        ## the space is vertical.
        "vertical"
    }?,
    
    ## (responsible party) indicates the individual responsible for identifying and measuring the space.
    attribute resp { sh_data.pointer }?,
    empty
  }
sh_subst =
  
  ## (substitution) groups one or more deletions with one or more additions when the combination is to be regarded as a single intervention in the text.
  element subst {
    ((sh_add | sh_del | sh_model.milestoneLike)+)
    >> sch:pattern [
         id = "subst-constraint-substContents1"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:subst"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "child::tei:add and child::tei:del"
             "\x{a}" ~
             "                  "
             sch:name [ ]
             " must have at least one child add and at least one child del"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    sh_att.global.attributes,
    sh_att.transcriptional.attributes,
    empty
  }
sh_substJoin =
  
  ## (substitution join) identifies a series of possibly fragmented additions, deletions or other revisions on a manuscript that combine to make up a single intervention in the text
  element substJoin {
    (sh_model.descLike | sh_model.certLike)*,
    sh_att.global.attributes,
    sh_att.pointing.attributes,
    sh_att.transcriptional.attributes,
    empty
  }
sh_supplied =
  
  ## signifies text supplied by the transcriber or editor for any reason, typically because the original cannot be read because of physical damage or loss to the original. [11.3.3.1. ]
  element supplied {
    sh_macro.paraContent,
    sh_att.global.attributes,
    sh_att.editLike.attributes,
    
    ## indicates why the text has had to be supplied.
    attribute reason {
      list { sh_data.word+ }
    }?,
    empty
  }
sh_surplus =
  
  ## marks text present in the source which the editor believes to be superfluous or redundant. [11.3.3.1. ]
  element surplus {
    sh_macro.paraContent,
    sh_att.global.attributes,
    sh_att.editLike.attributes,
    
    ## indicates the grounds for believing this text to be superfluous.
    attribute reason {
      list { sh_data.word+ }
    }?,
    empty
  }
sh_line =
  
  ## contains the transcription of a topographic line in the source document
  element line {
    (text | sh_model.global | sh_model.linePart)*,
    sh_att.typed.attributes,
    sh_att.global.attributes,
    sh_att.coordinated.attributes,
    empty
  }
sh_listTranspose =
  
  ## supplies a list of transpositions, each of which is indicated at some point in a document typically by means of metamarks.
  element listTranspose {
    sh_transpose+, sh_att.global.attributes, empty
  }
sh_metamark =
  
  ## contains or describes any kind of graphic or written signal within a document the function of which is to determine how it should be read rather than forming part of the actual content of the document.
  element metamark {
    sh_macro.specialPara,
    sh_att.spanning.attributes,
    sh_att.placement.attributes,
    sh_att.global.attributes,
    
    ## describes the function (for example status, insertion, deletion, transposition) of the mark.
    attribute function { sh_data.word }?,
    
    ## identifies one or more elements to which the function indicated by the metamark applies. 
    attribute target {
      list { sh_data.pointer+ }
    }?,
    empty
  }
sh_mod =
  
  ## represents any kind of modification identified within a single document.
  element mod {
    sh_macro.paraContent,
    sh_att.global.attributes,
    sh_att.transcriptional.attributes,
    sh_att.typed.attributes,
    sh_att.spanning.attributes,
    empty
  }
sh_redo =
  
  ## indicates one or more cancelled interventions in a document which have subsequently been marked as reaffirmed or repeated.
  element redo {
    empty,
    sh_att.global.attributes,
    sh_att.spanning.attributes,
    sh_att.transcriptional.attributes,
    
    ## points to one or more elements representing the interventions which are being reasserted.
    attribute target {
      list { sh_data.pointer+ }
    }?,
    empty
  }
sh_retrace =
  
  ## contains a sequence of writing which has been retraced, for example by over-inking, to clarify or fix it.
  element retrace {
    sh_macro.paraContent,
    sh_att.global.attributes,
    sh_att.spanning.attributes,
    sh_att.transcriptional.attributes,
    empty
  }
sh_transpose =
  
  ## describes a single textual transposition as an ordered list of at least two pointers specifying the order in which the elements indicated should be re-combined. 
  element transpose {
    (sh_ptr, sh_ptr+), sh_att.global.attributes, empty
  }
sh_undo =
  
  ## indicates one or more marked-up interventions in a document which have subsequently been marked for cancellation.
  element undo {
    empty,
    sh_att.global.attributes,
    sh_att.spanning.attributes,
    sh_att.transcriptional.attributes,
    
    ## points to one or more elements representing the interventions which are to be reverted or undone.
    attribute target {
      list { sh_data.pointer+ }
    }?,
    empty
  }
sh_table =
  
  ## contains text displayed in tabular form, in rows and columns. [14.1.1. ]
  element table {
    ((sh_model.headLike | sh_model.global)*,
     ((sh_row, sh_model.global*)+
      | (sh_model.graphicLike, sh_model.global*)+),
     (sh_model.divBottom, sh_model.global*)*),
    sh_att.global.attributes,
    
    ## indicates the number of rows in the table.
    attribute rows { sh_data.count }?,
    
    ## (columns) indicates the number of columns in each row of the table.
    attribute cols { sh_data.count }?,
    empty
  }
sh_row =
  
  ## contains one row of a table.  [14.1.1. ]
  element row {
    sh_cell+,
    sh_att.global.attributes,
    sh_att.tableDecoration.attributes,
    empty
  }
sh_cell =
  
  ## contains one cell of a table.  [14.1.1. ]
  element cell {
    sh_macro.paraContent,
    sh_att.global.attributes,
    sh_att.tableDecoration.attributes,
    empty
  }
sh_formula =
  
  ## contains a mathematical or other formula. [14.2. ]
  element formula {
    (text | sh_model.graphicLike | sh_model.hiLike)*,
    sh_att.global.attributes,
    
    ## names the notation used for the content of the element.
    attribute notation { sh_data.enumerated }?,
    empty
  }
sh_notatedMusic =
  
  ## encodes the presence of music notation in a text
  element notatedMusic {
    (sh_model.labelLike
     | sh_model.ptrLike
     | sh_graphic
     | sh_binaryObject)*,
    sh_att.global.attributes,
    sh_att.placement.attributes,
    sh_att.typed.attributes,
    empty
  }
sh_figure =
  
  ## groups elements representing or containing graphic information such as an illustration, formula, or figure. [14.4. ]
  element figure {
    (sh_model.headLike
     | sh_model.common
     | sh_figDesc
     | sh_model.graphicLike
     | sh_model.global
     | sh_model.divBottomPart
     | sh_iconClass)*,
    sh_att.global.attributes,
    sh_att.placement.attributes,
    sh_att.typed.attributes,
    empty
  }
sh_figDesc =
  
  ## (description of figure) contains a brief prose description of the appearance or content of a graphic figure, for use when documenting an image without displaying it. [14.4. ]
  element figDesc {
    sh_macro.limitedContent, sh_att.global.attributes, empty
  }
sh_g =
  
  ## (character or glyph) represents a glyph, or a non-standard character. [5. ]
  element g {
    text,
    sh_att.global.attributes,
    sh_att.typed.attributes,
    
    ## points to a description of the character or glyph intended.
    attribute ref { sh_data.pointer }?,
    empty
  }
sh_char =
  
  ## (character) provides descriptive information about a character. 
  element char {
    (sh_charName?,
     sh_model.descLike*,
     sh_charProp*,
     sh_mapping*,
     sh_figure*,
     sh_model.graphicLike*,
     sh_model.noteLike*),
    sh_att.global.attributes,
    empty
  }
sh_charName =
  
  ## (character name) contains the name of a character, expressed following Unicode conventions. 
  element charName { text, sh_att.global.attributes, empty }
sh_charProp =
  
  ## (character property) provides a name and value for some property of the parent character or glyph. 
  element charProp {
    ((sh_unicodeName | sh_localName), sh_value),
    sh_att.global.attributes,
    sh_att.typed.attributes,
    empty
  }
sh_charDecl =
  
  ## (character declarations) provides information about nonstandard characters and glyphs. 
  element charDecl {
    (sh_desc?, (sh_char | sh_glyph)+),
    sh_att.global.attributes,
    empty
  }
sh_glyph =
  
  ## (character glyph) provides descriptive information about a character glyph. 
  element glyph {
    (sh_glyphName?,
     sh_model.descLike*,
     sh_charProp*,
     sh_mapping*,
     sh_figure*,
     sh_model.graphicLike*,
     sh_model.noteLike*),
    sh_att.global.attributes,
    empty
  }
sh_glyphName =
  
  ## (character glyph name) contains the name of a glyph, expressed following Unicode conventions for character names. 
  element glyphName { text, sh_att.global.attributes, empty }
sh_localName =
  
  ## (locally-defined property name) contains a locally defined name for some property. 
  element localName { text, sh_att.global.attributes, empty }
sh_mapping =
  
  ## (character mapping) contains one or more characters which are related to the parent character or glyph in some respect, as specified by the type attribute.
  element mapping {
    sh_macro.xtext,
    sh_att.global.attributes,
    sh_att.typed.attributes,
    empty
  }
sh_unicodeName =
  
  ## (unicode property name) contains the name of a registered Unicode normative or informative property. 
  element unicodeName {
    text,
    sh_att.global.attributes,
    
    ## specifies the version number of the Unicode Standard in which this property name is defined.
    attribute version { sh_data.version }?,
    empty
  }
sh_value =
  
  ## (value) contains a single value for some property, attribute, or other analysis.
  element value { sh_macro.xtext, sh_att.global.attributes, empty }
sh_s =
  
  ## (s-unit) contains a sentence-like division of a text. [17.1.  8.4.1. ]
  element s {
    sh_macro.phraseSeq
    >> sch:pattern [
         id = "s-constraint-noNestedS"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:s"
           "\x{a}" ~
           "               "
           sch:report [
             test = "tei:s"
             "You may not nest one s element within\x{a}" ~
             "      another: use seg instead"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    sh_att.global.attributes,
    sh_att.segLike.attributes,
    sh_att.typed.attributes,
    empty
  }
sh_cl =
  
  ## (clause) represents a grammatical clause. [17.1. ]
  element cl {
    sh_macro.phraseSeq,
    sh_att.global.attributes,
    sh_att.segLike.attributes,
    sh_att.typed.attributes,
    empty
  }
sh_phr =
  
  ## (phrase) represents a grammatical phrase. [17.1. ]
  element phr {
    sh_macro.phraseSeq,
    sh_att.global.attributes,
    sh_att.segLike.attributes,
    sh_att.typed.attributes,
    empty
  }
sh_w =
  
  ## (word) represents a grammatical (not necessarily orthographic) word. [17.1. ]
  element w {
    (text
     | sh_model.gLike
     | sh_w
     | sh_m
     | sh_c
     | sh_model.global
     | sh_model.lPart
     | sh_model.hiLike
     | sh_model.pPart.edit)*,
    sh_att.global.attributes,
    sh_att.segLike.attributes,
    sh_att.typed.attributes,
    
    ## provides a lemma for the word, such as an uninflected dictionary entry form.
    attribute lemma { sh_data.text }?,
    
    ## provides a pointer to a definition of the lemma for the word, for example in an online lexicon. 
    attribute lemmaRef { sh_data.pointer }?,
    empty
  }
sh_m =
  
  ## (morpheme) represents a grammatical morpheme. [17.1. ]
  element m {
    (text | sh_model.gLike | sh_m | sh_c | sh_model.global)*,
    sh_att.global.attributes,
    sh_att.segLike.attributes,
    sh_att.typed.attributes,
    
    ## identifies the morpheme's base form.
    attribute baseForm { sh_data.word }?,
    empty
  }
sh_c =
  
  ## (character) represents a character. [17.1. ]
  element c {
    sh_macro.xtext,
    sh_att.global.attributes,
    sh_att.segLike.attributes,
    sh_att.typed.attributes,
    empty
  }
sh_pc =
  
  ##  (punctuation character) contains a character or string of characters regarded as constituting a single punctuation mark. [17.1. ]
  element pc {
    (text | sh_model.gLike | sh_c | sh_model.pPart.edit)*,
    sh_att.global.attributes,
    sh_att.segLike.attributes,
    sh_att.typed.attributes,
    
    ## indicates the extent to which this punctuation mark conventionally separates words or phrases
    attribute force {
      
      ## the punctuation mark is a word separator
      "strong"
      | 
        ## the punctuation mark is not a word separator
        "weak"
      | 
        ## the punctuation mark may or may not be a word separator
        "inter"
    }?,
    
    ## provides a name for the kind of unit delimited by this punctuation mark.
    attribute unit { sh_data.enumerated }?,
    
    ## indicates whether this punctuation mark precedes or follows the unit it delimits.
    attribute pre { sh_data.truthValue }?,
    empty
  }
sh_att.global.analytic.attributes = sh_att.global.analytic.attribute.ana
sh_att.global.analytic.attribute.ana =
  
  ## (analysis) indicates one or more elements containing interpretations of the element on which the ana attribute appears.
  attribute ana {
    list { sh_data.pointer+ }
  }?
sh_span =
  
  ## associates an interpretative annotation directly with a span of text. [17.3. ]
  element span {
    sh_macro.phraseSeq.limited
    >> sch:pattern [
         id = "span-constraint-targetfrom"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:span"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@from and @target"
             "\x{a}" ~
             "Only one of the attributes @target and @from may be supplied on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "span-constraint-targetto"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:span"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@to          and          @target"
             "\x{a}" ~
             "Only one of the attributes @target and @to may be supplied on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "span-constraint-tonotfrom"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:span"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@to          and          not(@from)"
             "\x{a}" ~
             "If @to is supplied on "
             sch:name [ ]
             ", @from must be supplied as well"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "span-constraint-tofrom"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:span"
           "\x{a}" ~
           "               "
           sch:report [
             test = "contains(@to,' ') or contains(@from,' ')"
             "\x{a}" ~
             "The attributes @to and @from on "
             sch:name [ ]
             " may each contain only a single value"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    sh_att.global.attributes,
    sh_att.interpLike.attributes,
    sh_att.pointing.attributes,
    
    ## gives the identifier of the node which is the starting point of the span of text being annotated; if not accompanied by a to attribute, gives the identifier of the node of the entire span of text being annotated.
    attribute from { sh_data.pointer }?,
    
    ## gives the identifier of the node which is the end-point of the span of text being annotated.
    attribute to { sh_data.pointer }?,
    empty
  }
sh_spanGrp =
  
  ## (span group) collects together span tags. [17.3. ]
  element spanGrp {
    sh_span*,
    sh_att.global.attributes,
    sh_att.interpLike.attributes,
    empty
  }
sh_interp =
  
  ## (interpretation) summarizes a specific interpretative annotation which can be linked to a span of text. [17.3. ]
  element interp {
    (text | sh_model.gLike | sh_model.descLike | sh_model.certLike)*,
    sh_att.global.attributes,
    sh_att.interpLike.attributes,
    empty
  }
sh_interpGrp =
  
  ## (interpretation group) collects together a set of related interpretations which share responsibility or type. [17.3. ]
  element interpGrp {
    (sh_model.descLike*, sh_interp+),
    sh_att.global.attributes,
    sh_att.interpLike.attributes,
    empty
  }
sh_iconClass =
  
  ## This element is the ICONCLASS assigned to the images contained in the woodcut. The name element contains the ICONCLASS code. The desc element contains a text ICONCLASS associates with the code.
  element ns2:iconClass {
    sh_name, sh_desc, sh_att.global.attributes, empty
  }
start = sh_TEI | sh_teiCorpus
