    [ a:defaultValue = "unknown" ]
    attribute aloud { sh_data.xTruthValue }?,
    
    ## may be used to indicate whether the quoted matter is regarded as direct or indirect speech.
    [ a:defaultValue = "true" ]
    attribute direct { sh_data.xTruthValue }?,
